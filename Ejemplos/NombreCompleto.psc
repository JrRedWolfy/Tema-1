Algoritmo NombreCompleto
	
	
	Definir nombre como caracter;
	Definir apellido1 Como Caracter;
	Definir apellido2 Como Caracter;
	
	Escribir "Cual es tu nombre";
	Leer nombre;
	Escribir "Y tu primer apellido";
	Leer apellido1;
	Escribir "Y tu tercer apellido(Dejar campo en blanco si no se tiene)";
	Leer apellido2;
	Escribir "Tu nombre es: ", nombre," ",apellido1," ",apellido2;
	
	
	
	
FinAlgoritmo
