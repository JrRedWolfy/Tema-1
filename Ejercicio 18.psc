Algoritmo multiplios
	Definir multi Como enteros;
	multi = 2;
	
	
	Escribir "Multiplos de 2 desde el 1 al 100:";
	Mientras multi <= 100 Hacer
		Escribir sin saltar multi, ", ";
		multi = multi + 2;
	Fin Mientras
	Escribir "";
	
	
	multi = 3;
	Escribir "Multiplos de 3 desde el 1 al 100:";
	Mientras multi <= 100 Hacer
		Escribir sin saltar multi, ", ";
		multi = multi + 3;
	Fin Mientras
	Escribir "";
	
FinAlgoritmo
